{
  "name": "sqrl-min-auth",
  "version": "0.5.0",
  "description": "",
  "engines": {
    "node": "12"
  },
  "main": "src/index.js",
  "scripts": {
    "db:migrate": "sls invoke -f dbMigrate --stage dev",
    "deploy": "sls deploy --stage dev",
    "local": "serve public",
    "format": "prettier --write '{src/**/*.{js,json,graphql,md},*.md,package.json,*.js}'",
    "start": "node src/sqrl.js | pino-pretty",
    "start:db": "docker run --rm -it -p 5432:5432 -e POSTGRES_USER=sqrl -e POSTGRES_PASSWORD=sqrl -e POSTGRES_DB=sqrl postgres:latest"
  },
