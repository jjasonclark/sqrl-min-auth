    "cookie-signature": "^1.1.0",
    "dlv": "^1.1.3",
    "pg": "^7.12.1",
    "pg-promise": "^9.3.3",
    "pino": "^5.13.5",
    "serverless": "^1.56.1",
    "tweetnacl": "^1.0.1",
    "universal-base64url": "^1.1.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "pino-pretty": "^3.2.2",
    "prettier": "^1.18.2",
    "serve": "^11.2.0"
