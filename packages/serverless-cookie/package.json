{
  "name": "sqrl-min-auth",
  "version": "0.5.0",
  "description": "",
  "engines": {
    "node": "12"
  },
  "main": "src/index.js",
  "scripts": {
    "db:migrate": "sls invoke -f dbMigrate --stage dev",
    "deploy": "sls deploy --stage dev",
    "local": "serve public",
    "format": "prettier --write '{src/**/*.{js,json,graphql,md},*.md,package.json,*.js}'",
    "start": "node src/sqrl.js | pino-pretty",
    "start:db": "docker run --rm -it -p 5432:5432 -e POSTGRES_USER=sqrl -e POSTGRES_PASSWORD=sqrl -e POSTGRES_DB=sqrl postgres:latest"
  },
  "keywords": [],
  "author": "Jason Clark <jason@jjasonclark.com> (https://jjasonclark.com)",
  "license": "MIT",
  "dependencies": {
    "cookie-signature": "^1.1.0",
    "cookie": "^0.4.0",
    "dlv": "^1.1.3",
    "pg": "^7.14.0",
    "pg-promise": "^10.3.1",
    "pg-sqrl-store": "file:../pg-sqrl-store",
    "pino": "^5.14.0",
    "sqrl-protocol": "file:../sqrl-protocol"
  },
  "devDependencies": {
    "pino-pretty": "^3.4.0",
    "prettier": "^1.19.1",
    "serve": "^11.2.0",
    "serverless-prune-plugin": "^1.4.1",
    "serverless": "^1.56.1"
  }
